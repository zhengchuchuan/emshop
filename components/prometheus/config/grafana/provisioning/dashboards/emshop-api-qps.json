  "panels": [
    {
      "type": "stat",
      "title": "Overall QPS",
      "id": 1,
      "gridPos": { "h": 6, "w": 6, "x": 0, "y": 0 },
      "datasource": {
        "type": "prometheus",
        "uid": "$DS_PROMETHEUS"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(http_server_requests_emshop_total{route!=\"/metrics\"}[1m]))",
          "legendFormat": "QPS"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "horizontal",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "req/s",
          "thresholds": { "mode": "absolute", "steps": [ {"color": "green"}, {"color": "red", "value": 1000} ] }
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "QPS by Service",
      "id": 2,
      "gridPos": { "h": 6, "w": 18, "x": 6, "y": 0 },
      "datasource": {
        "type": "prometheus",
        "uid": "$DS_PROMETHEUS"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (service) (rate(http_server_requests_emshop_total{route!=\"/metrics\"}[1m]))",
          "legendFormat": "{{service}}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "req/s" }, "overrides": [] },
      "options": { "legend": { "showLegend": true }, "tooltip": { "mode": "multi" } }
    },
    {
      "type": "timeseries",
      "title": "Top Routes QPS (by Service)",
      "id": 3,
      "gridPos": { "h": 9, "w": 12, "x": 0, "y": 6 },
      "datasource": {
        "type": "prometheus",
        "uid": "$DS_PROMETHEUS"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(10, sum by (route) (rate(http_server_requests_emshop_total{service=~\"$service\", route!=\"/metrics\"}[1m])))",
          "legendFormat": "{{route}}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "req/s" }, "overrides": [] },
      "options": { "legend": { "showLegend": true }, "tooltip": { "mode": "multi" } }
    },
    {
      "type": "timeseries",
      "title": "QPS by Method (Service=$service, Route=$route)",
      "id": 4,
      "gridPos": { "h": 9, "w": 12, "x": 12, "y": 6 },
      "datasource": {
        "type": "prometheus",
        "uid": "$DS_PROMETHEUS"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (method) (rate(http_server_requests_emshop_total{service=~\"$service\", route=~\"$route\", route!=\"/metrics\"}[1m]))",
          "legendFormat": "{{method}}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "req/s" }, "overrides": [] },
      "options": { "legend": { "showLegend": true }, "tooltip": { "mode": "multi" } }
    },
    {
      "type": "timeseries",
      "title": "Error QPS by Code (Service=$service)",
      "id": 5,
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 15 },
      "datasource": {
        "type": "prometheus",
        "uid": "$DS_PROMETHEUS"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (code) (rate(http_server_requests_emshop_total{service=~\"$service\", code!~\"2..\", route!=\"/metrics\"}[5m]))",
          "legendFormat": "{{code}}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "req/s" }, "overrides": [] },
      "options": { "legend": { "showLegend": true }, "tooltip": { "mode": "multi" } }
    }
  ],
